#Duck Typing

class vscode:
    def execute(self):
        print("Compiling")
        print("Running")
class Laptop:
    def code(self, ide):
        ide.execute()
ide = vscode()
lap1 = Laptop()
lap1.code(ide)


class vscode:
    def execute(self):
        print("Compiling")
        print("Running")
        
class MyEditor:
    def execute(self):
        print("Spell Check")
        print("Convention Check")
        print("compiling")
        print("Running")
        
class Laptop:
    def code(self, ide):
        ide.execute()
ide = vscode()
ide = MyEditor()
lap1 = Laptop()
lap1.code(ide)

#operator overloading     
