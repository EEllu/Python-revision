c1.age = 12
c1.update()
print(c1.name)
print(c2.name)


class Computer:
    def __init__(self):
        self.name = "Aarjav"
        self.age = 24
    def update(self):
        self.age = 23
        
    def compare(self, other): #compare(who is calling, whom to compare)
        if self.age == other.age:
            return True
        else:
            return False
        
c1 = Computer()
c1.age = 30
c2 = Computer()

if c1.compare(c2):
    print("They are same")
else:
    print("They are different")


print(c1.name)
print(c2.name)


