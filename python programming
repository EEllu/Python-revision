#iterator = It is used to get one value at a time from a list of values.
nums = [7,8,9,5]
for i in nums:
    print(i)

nums = [7,8,9,5]
it = iter(nums)
print(it.__next__()) # it will give the first value.
print(it.__next__()) # again next value is printed
#or
print(next(it))


class TopTen: 
    def __init__(self):
        self.num = 1
    def __iter__(self):
        return self
    def __next__(self):
        if self.num <= 10:
            value = self.num
            self.num += 1
            return value
        else:
            raise StopIteration

values = TopTen()
for i in values:
    print(i)


#Generator 

def TopTen():
    n = 1
    while n <= 10:
        sq = n * n
        yield sq #yield is used in generators
        n += 1
values = TopTen()
for i in values:
    print(i)

# Exception Handling is used to handle bugs or exceptions.

a = 5
b = 0
try:
    print(a/b)
except Exception as e: # except will execute only if you have an error
    print("Hey you cannot divide a number by zero!", e)
    
print("Bye")


a = 5
b = 0
try:
    print("resource open")
    print(a/b)
except Exception as e: # except will execute only if you have an error
    print("Hey you cannot divide a number by zero!")
    
finally:
    print("resource closed")



a = 5
b = 0

try:
    print("resource open")
    print(a/b)
    k = int(input("Enter a number: "))
    print(k)
    
except ZeroDivisionError as e:
    print("Hey you cannot divide a number by zero!",e)
    
except ValueError as e:
    print("Invalid input")

except Exception as e:
    print("something went wrong...")
    
finally:
    print("resource closed")
    
    
#MultiThreading
from threading import *
class Hello(Thread):    # thread is a method imported from threading.
    def run (self):
        for i in range(5):
            print("Hello!")
            
class Hi(Thread):
    def run (self):
        for i in range(5):
            print("Hi!")
t1 = Hello()
t2 = Hi()

t1.run()
t2.run()


from time import *
from threading import *
class Hello(Thread):    
    def run (self):
        for i in range(5):
            print("Hello!")
            sleep(1) # sleep is a method imported from time.
            
class Hi(Thread):
    def run (self):
        for i in range(5):
            print("Hi!")
            sleep(1)
t1 = Hello()
t2 = Hi()

t1.start()
sleep(0.2)
t2.start()



from time import *
from threading import *
class Hello(Thread):    
    def run (self):
        for i in range(5):
            print("Hello!")
            sleep(1) # sleep is a method imported from time.
            
class Hi(Thread):
    def run (self):
        for i in range(5):
            print("Hi!")
            sleep(1)
t1 = Hello()
t2 = Hi()

t1.start()
sleep(0.2)
t2.start()

t1.join() # join is used to make main thread wait for other threads to join and then main thread will execute.
t2.join()

print("Bye")




