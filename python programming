#copying an array.
arr1 = array([1,2,3,4,5])
arr2 = arr1
print(arr1)
print(arr2)
print(id(arr1))
print(id(arr2))

#shallow copying (2 arrays linked to each other)
arr1 = array([2,6,8,1,3])
arr2 = arr1.view() #.view gives shallow copy
arr1[1] = 7
print(arr1)
print(arr2)
print(id(arr1))
print(id(arr2))

#deep copying (2 arrays not linked together)

arr1 = array([2,6,8,1,3])
arr2 = arr1.copy() # .copy gives deep copy.
arr1[1] = 7
print(arr1)
print(arr2)
print(id(arr1))
print(id(arr2))

