#Special method __init__ method 

class Computer: 
    def __init__(self, cpu, ram): #  here object is self
        self.cpu = cpu
        self.ram = ram
    def config(self):
        print("config is: ", self.cpu, self.ram)
        
com1 = Computer('i5',16)
com2 = Computer('Ryzen 3', 8)

com1.config()
com2.config()

#constructor, self and comparing objects.
class Computer:
    def __init__(self):
        self.name = "Aarjav"
        self.age = 24
    def update(self):
        self.age = 23
